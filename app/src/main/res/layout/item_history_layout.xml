<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="29dp">

    <TextView
        android:id="@+id/tv_history_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="11dp"
        android:text="denglubushang"
        android:textColor="@color/main_text_col"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/tv_history_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2018-05-06"
        android:textSize="8sp"
        android:textColor="@color/main_seek_text_col"
        android:layout_centerInParent="true"/>
    <ImageView
        android:id="@+id/iv_history_del"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/login_delete_icon"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="12dp"/>
</RelativeLayout>
