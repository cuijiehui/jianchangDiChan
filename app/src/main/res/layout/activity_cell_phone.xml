<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/like_back_col"
    tools:context="com.cui.android.jianchengdichan.view.ui.avtivity.CellPhoneActivity">
<include layout="@layout/include_top_layout"/>
    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_cell_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"/>
    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_cell_data"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>
